{
    "version": 2,
    "name": "practicecoding-backend",
    "github": {
        "autoAlias": false
    },
    "builds": [{
        "src": "./src/**/*.ts",
        "use": "@now/node@canary"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/src/$1"
    }],
    "env": {
        "NODE_ENV": "production"
    }
}