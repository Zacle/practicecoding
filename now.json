{
    "version": 2,
    "name": "practicecoding-backend",
    "github": {
        "autoAlias": false
    },
    "builds": [{
        "src": "dist/server.js",
        "use": "@now/node-server",
        "config": {
            "useBuildUtils": "@now/build-utils@canary"
        }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/dist/$1"
    }],
    "env": {
        "NODE_ENV": "production"
    }
}