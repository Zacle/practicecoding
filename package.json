    "@tsed/mongoose": "^5.12.0",
    "@tsed/swagger": "^5.12.0",
    "@tsed/testing": "^5.12.0",
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/csv-parse": "^1.1.12",
    "@types/facebook-js-sdk": "^3.2.1",
    "@types/jszip": "^3.1.5",
